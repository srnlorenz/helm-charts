        matchLabels:
          app: mlflow
          app.kubernetes.io/instance: mlflow
          app.kubernetes.io/name: mlflow
      strategy:
        rollingUpdate:
          maxSurge: 100%
          maxUnavailable: 0
        type: RollingUpdate
      template:
        metadata:
